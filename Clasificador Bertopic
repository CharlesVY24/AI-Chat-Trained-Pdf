        "    umap_model=umap_model,\n",
        "    hdbscan_model=hdbscan_model,\n",
        "    representation_model=representation_model,\n",
        "    top_n_words=10,\n",
        "    verbose=True,\n",
        ")\n",
        "\n",
        "topics, probs = topic_model.fit_transform(docs, embeddings)\n"
      ]
