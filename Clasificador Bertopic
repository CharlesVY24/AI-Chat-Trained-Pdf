        "from sklearn.neighbors import NearestNeighbors\n",
        "import numpy as np\n",
        "\n",
        "nn = NearestNeighbors(n_neighbors=15, metric=\"cosine\").fit(embeddings)\n",
        "distances, _ = nn.kneighbors(embeddings)\n",
        "\n",
        "# Look at mean distance to nearest neighbors\n",
        "print(\"Mean NN distance:\", np.mean(distances[:, 1:]))\n",
        "print(\"Median NN distance:\", np.median(distances[:, 1:]))\n"
      ]
    },
