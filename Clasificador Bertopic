      "outputs": [],
      "source": [
        "\n",
        "\n",
        "# 🔹 2D embeddings for visualization (optional)\n",
        "reduced_embeddings = UMAP(\n",
        "    n_neighbors=15,\n",
        "    n_components=2,\n",
        "    min_dist=0.0,\n",
        "    metric='cosine',\n",
        "    random_state=42\n",
        ").fit_transform(embeddings)\n",
        "\n",
        "# 🔹 Sub-models for BERTopic\n",
        "umap_model = UMAP(\n",
        "    n_neighbors=15,\n",
        "    n_components=5,\n",
        "    min_dist=0.0,\n",
        "    metric='cosine',\n",
        "    random_state=42\n",
        ")\n",
        "hdbscan_model = HDBSCAN(\n",
        "    min_cluster_size=400,          # you may want to reduce this if you have fewer documents\n",
        "    metric='euclidean',\n",
        "    cluster_selection_method='eom',\n",
        "    prediction_data=True\n",
        ")\n"
      ]
    },
