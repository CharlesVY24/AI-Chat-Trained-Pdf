      "cell_type": "code",
      "source": [
        "from sklearn.neighbors import NearestNeighbors\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "\n",
        "# Candidate values to test\n",
        "candidate_neighbors = [5, 10, 15, 20, 25, 30, 40, 50]\n",
        "\n",
        "results = []\n",
        "\n",
        "for k in candidate_neighbors:\n",
        "    nn = NearestNeighbors(n_neighbors=k, metric=\"cosine\").fit(embeddings)\n",
        "    distances, _ = nn.kneighbors(embeddings)\n",
        "\n",
        "    # Exclude first column (distance to itself = 0)\n",
        "    mean_dist = np.mean(distances[:, 1:])\n",
        "    median_dist = np.median(distances[:, 1:])\n",
        "    std_dist = np.std(distances[:, 1:])\n",
        "\n",
        "    results.append({\n",
        "        \"n_neighbors\": k,\n",
        "        \"mean_distance\": mean_dist,\n",
        "        \"median_distance\": median_dist,\n",
        "        \"std_distance\": std_dist\n",
        "    })\n",
        "\n",
        "# Convert to DataFrame for readability\n",
        "df_results = pd.DataFrame(results)\n",
        "print(df_results)\n"
      ],
